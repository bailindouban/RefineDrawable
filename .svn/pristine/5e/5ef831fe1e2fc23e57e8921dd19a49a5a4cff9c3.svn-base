package com.asus.comparator;

import java.util.Comparator;

import com.asus.obj.Property;

/**
 * Ordered by orientation
 * @author Kim_Bai
 *
 */

public class ComparatorByOri implements Comparator<Property> {
	
	@Override
	public int compare(Property p1, Property p2) {
		// TODO Auto-generated method stub
		if (p1.smallestWidth.equals(p2.smallestWidth)) {
			return p2.orientation.compareTo(p1.orientation);
		}
		
		return 0;
	}
}
